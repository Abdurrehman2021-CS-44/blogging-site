<%- include("partials/header") -%>

    <h1>Home</h1>
    <p><%= startingContent %></p>
    <% allPosts.forEach(function(post){ %>
        <h1><%= post.title %></h1>
        <% var link = "/posts/" + post._id %>
        <p><%= post.body.substr(0, 100) + " ..." %> <a href=<%= link %>>Read More</a> </p>
    <% }); %>
    <div class="for-spacing"></div>

<%- include("partials/footer") -%>