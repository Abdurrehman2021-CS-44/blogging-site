<%- include("partials/header") -%>

    <h1>Home</h1>
    <p><%= startingContent %></p>
    <div class="blogs">
        <% allPosts.forEach(function(post){ %>
            <h1><%= post.title %></h1>
            <% var link = "/posts/" + post._id %>
            <% console.log(link) %>
            <p><%= post.body.substr(0, 100) + " ..." %> <a href=<%= link %>>Read More</a> </p>
        <% }); %>
    </div>

<%- include("partials/footer") -%>